{
  "stash": {
    "settings": {
      "index": {
        "refresh_interval": "3s",
        "number_of_shards": "1",
        "number_of_replicas": "0",
        "requests.cache.enable": "true",
        "translog.durability": "async",
        "translog.sync_interval": "15s"
      }
    },
    "mappings": {
      "stash": {
        "properties": {
          "id": {
            "type": "keyword"
          },
          "league": {
            "type": "keyword"
          },
          "name": {
            "type": "text"
          },
          "lastSeen": {
            "type": "date"
          },
          "seller": {
            "properties": {
              "account": {
                "type": "keyword"
              },
              "character": {
                "type": "keyword"
              }
            }
          }
        }
      }
    }
  },
  "listing": {
    "settings": {
      "index": {
        "refresh_interval": "15s",
        "number_of_shards": "1",
        "number_of_replicas": "0",
        "requests.cache.enable": "true",
        "translog.durability": "async",
        "translog.sync_interval": "30s"
      },
      "analysis": {
        "analyzer": {
          "ngram": {
            "tokenizer": "ngram"
          },
          "analyzer_keyword": {
            "tokenizer": "keyword",
            "filter": "lowercase"
          }
        },
        "tokenizer": {
          "ngram": {
            "type": "nGram",
            "min_gram": "4",
            "max_gram": "8",
            "token_chars": [
              "letter",
              "digit",
              "whitespace",
              "punctuation",
              "symbol"
            ]
          }
        }
      }
    },
    "mappings": {
      "listing": {
        "properties": {
          "id": {
            "type": "keyword"
          },
          "stash": {
            "properties": {
              "id": {
                "type": "keyword"
              },
              "x": {
                "type": "integer"
              },
              "y": {
                "type": "integer"
              }
            }
          },
          "size": {
            "properties": {
              "width": {
                "type": "integer"
              },
              "height": {
                "type": "integer"
              }
            }
          },
          "league": {
            "type": "keyword"
          },
          "name": {
            "type": "text"
          },
          "fullName": {
            "type": "text"
          },
          "itemType": {
            "type": "keyword"
          },
          "gearType": {
            "type": "keyword"
          },
          "baseLine": {
            "type": "text"
          },
          "rarity": {
            "type": "keyword"
          },
          "quality": {
            "type": "integer"
          },
          "note": {
            "type": "text",
            "index": "not_analyzed"
          },
          "level": {
            "type": "integer"
          },
          "tier": {
            "type": "integer"
          },
          "identified": {
            "type": "boolean"
          },
          "corrupted": {
            "type": "boolean"
          },
          "icon": {
            "type": "text",
            "index": "not_analyzed"
          },
          "iconVersion": {
            "type": "text",
            "index": "not_analyzed"
          },
          "flavourText": {
            "type": "text",
            "index": "not_analyzed"
          },
          "price": {
            "properties": {
              "raw": {
                "type": "text",
                "index": "not_analyzed"
              },
              "chaos": {
                "type": "float"
              }
            }
          },
          "stack": {
            "properties": {
              "count": {
                "type": "integer"
              },
              "maximum": {
                "type": "integer"
              }
            }
          },
          "firstSeen": {
            "type": "date"
          },
          "lastSeen": {
            "type": "date"
          },
          "lastParsed": {
            "type": "date"
          },
          "removed": {
            "type": "boolean"
          },
          "sockets": {
            "properties": {
              "red": {
                "type": "integer"
              },
              "green": {
                "type": "string"
              },
              "blue": {
                "type": "integer"
              },
              "white": {
                "type": "integer"
              },
              "links": {
                "type": "integer"
              }
            }
          },
          "requirements": {
            "properties": {
              "level": {
                "type": "integer"
              },
              "str": {
                "type": "integer"
              },
              "int": {
                "type": "integer"
              },
              "dex": {
                "type": "integer"
              }
            }
          },
          "modifiers": {
            "type": "text"
          },
          "meta": {
            "properties": {
              "quality": {
                "type": "float"
              },
              "level": {
                "type": "integer"
              },
              "prefix": {
                "type": "boolean"
              },
              "suffix": {
                "type": "boolean"
              }
            }
          },
          "offense": {},
          "defense": {},
          "stats": {}
        }
      }
    }
  },
  "league": {
    "settings": {
      "index": {
        "number_of_shards": "1",
        "number_of_replicas": "0"
      }
    },
    "mappings": {
      "league": {
        "properties": {
          "id": {
            "type": "keyword"
          },
          "url": {
            "type": "keyword"
          },
          "startAt": {
            "type": "date"
          },
          "endAt": {
            "type": "date"
          }
        }
      }
    }
  },
  "stats": {
    "settings": {
      "index": {
        "number_of_shards": "2"
      }
    },
    "mappings": {
      "fetch": {
        "properties": {
          "fileSizeKb": {
            "type": "float"
          },
          "downloadTimeMs": {
            "type": "float"
          },
          "timestamp": {
            "type": "date"
          }
        }
      }
    }
  }
}
